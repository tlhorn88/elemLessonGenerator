State('');\n  const [selectedMelodicConcept, setSelectedMelodicConcept] = useState('');\n\n  let kinderSequence = [];\n  let rhythmicSequence = [];\n  let melodicSequence = [];\n\n  conceptSequence.forEach((concept) => {\n    if (concept.conceptType === 'kinder') {\n      kinderSequence.push(concept);\n    } else if (concept.conceptType === 'rhythm') {\n      rhythmicSequence.push(concept);\n    } else if (concept.conceptType === 'melody') {\n      melodicSequence.push(concept);\n    }\n  });\n\n  // EVENT HANDLERS\n  const handleGenerateButtonClick = () => {\n    navigate(`/lessonDisplay?melodicConcept=${selectedMelodicConcept}`);\n  };\n\n  const handleMelodicStartingPointClick = (selectedConcept) => {\n    setSelectedMelodicConcept(selectedConcept);\n  };\n\n  const handleRhythmicStartingPointClick = (selectedConcept) => {\n    setSelectedRhythmicConcept(selectedConcept);\n  };\n\n  return (\n    <div>\n      <h1>LESSON PLAN GENERATOR</h1>\n      <div className=\"lessonPlanGrid\">\n        <div className=\"demographics\">\n          <h2>Student Demographics</h2>\n          <h3>Grade</h3>\n          <select>\n            <option>0</option>\n            <option>1</option>\n            <option>2</option>\n            <option>3</option>\n            <option>4</option>\n            <option>5</option>\n          </select>\n          <h3>Represented Languages</h3>\n          <h3>English</h3>\n          <h3>Spanish</h3>\n          <p>Add more</p>\n        </div>\n\n        <div className=\"advanced\">\n          <h2>Schedule</h2>\n          <p>\n            I see my students for *** minute lessons, *** times a week for ***\n            weeks a semester.\n          </p>\n        </div>\n\n        <div className=\"content\">\n          <h2>Content Options:</h2>\n          <h3>Starting points:</h3>\n          <h4>Melodic</h4>\n\n          <ul>\n            {melodicSequence.map((concept) => (\n              <li\n                className={`concept ${selectedMelodicConcept === concept.concept ? 'selected' : ''}`}\n                key={concept.id}\n                onClick={() => handleMelodicStartingPointClick(concept.concept)}\n              >\n                {concept.concept}\n              </li>\n            ))}\n          </ul>\n\n          <h3>Ending points:</h3>\n          <h4>Melodic</h4>\n          <h4>Rhythmic</h4>\n        </div>\n\n        <div className=\"conceptSequence\">\n          <h2>Concept Sequence</h2>\n          <p>(modify)</p>\n          <h4>Rhythmic</h4>\n          <ul>\n            {rhythmicSequence.map((concept) => (\n              <li\n              className='concept'\n              key={concept.id}\n              onClick={() => handleRhythmicStartingPointClick(concept.concept)}\n              >\n                {concept.concept}\n              </li>\n            ))}\n          </ul>\n        </div>\n      </div>\n      <button onClick={handleGenerateButtonClick}>GENERATE!</button>\n    </div>\n  );\n}\n\nexport default LessonPlanGenerator;\n","/Users/tabethahorn/Documents/code/elementaryMusic/src/Components/RepSearch/RepSearch.js",["109","110","111"],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/Components/ConceptSequence/ConceptSequence.js",["112"],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/redux/reducers.js",[],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/redux/conceptSequenceReducer.js",[],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/redux/holdingReducer.js",[],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/redux/lessonTemplateReducer.js",[],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/Components/LessonDisplay/LessonDisplay.js",["113","114","115","116","117","118"],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/utilities.js",[],[],"/Users/tabethahorn/Documents/code/elementaryMusic/src/redux/practiceSongsReducer.js",[],[],{"ruleId":"119","severity":1,"message":"120","line":8,"column":9,"nodeType":"121","messageId":"122",